{
    "version": 2,
    "builds": [
      {
        "src": "./backend/app.js",  
        "use": "@vercel/node"
      },
      {
        "src": "./frontend/dist",     
        "use": "@vercel/static"
      }
    ],
  
  
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "./backend/app.js"
      }
    ]
  }